  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python",
        "punctuation.definition.tag",
        "keyword.operator",
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FF5263"
      }
    },
    {
      "scope": [
        "entity.name.tag.css",
        "entity.name.tag.less",
        "storage",
        "storage.type",
        "storage.modifier",
        "keyword.operator.noexcept",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike",
        "keyword",
        "keyword.control",
        "keyword.other.unit",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        "variable.language"
      ],
      "settings": {
        "foreground": "#6c95eb"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#39cc8f"
      }
    },
    {
      "scope": [
        "constant.language",
        "entity.other.attribute-name",
        "entity.other.attribute-name.class.css",
        "source.css entity.other.attribute-name.class",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.parent.less",
        "source.css entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss",
        "meta.structure.dictionary.key.python",
        "string.tag",
        "support.type.vendored.property-name",
        "support.type.property-name",
        "source.css variable",
        "source.coffee.embedded",
        "variable.other.property",
        "variable.other.field",
        "variable.other.member"
      ],
      "settings": {
        "foreground": "#66c3cc"
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly",
        "meta.preprocessor.string",
        "string.value"
      ],
      "settings": {
        "foreground": "#c9a26d"
      }
    },
    // ===== [ TIP OF THE DAY ] =====
    // if token scope variable not showing, just set all of the attr they give and check token again it will show!

    // custom class in HTLM
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#E0B0FF"
      }
    },
    // custom type in typesrcipt
    {
      "scope": "meta.type.annotation.ts",
      "settings": {
        "foreground": "#6c95eb"
      }
    },
    // custom int
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ED94C0"
      }
    },
    // custom const (boolean, null, nil)
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#6c95eb"
      }
    },
    // custom typescript type annotation
    {
      "scope": "support.type.primitive.tsx",
      "settings": {
        "foreground": "#6c95eb"
      }
    },
    // custom type in generic -> <string>
    {
      "scope": "support.type.primitive.ts",
      "settings": {
        "foreground": "#6c95eb"
      }
    },
    // custom golang error interface
    {
      "scope": "storage.type.error.go",
      "settings": {
        "foreground": "#c191ff"
      }
    },
    // custom golang pink
    {
      "scope": [
        // go package name
        "entity.name.type.package.go",
        // go import var
        "variable.other.import.go",
        // string %s, %d
        "constant.character.escape.go",
        "constant.other.placeholder.go"
      ],
      "settings": {
        "foreground": "#E1BFFF"
      }
    },
    // custom docker-compose entity color
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#66c3cc"
      }
    },
    {
      "scope": [
        "variable.other.enummember",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent",
        "meta.preprocessor.numeric"
      ],
      "settings": {
        "foreground": "#51C0CF"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#767982"
      }
    },
    {
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#648769"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded",
        "string.regexp",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#B8B167"
      }
    },
    {
      "scope": [
        "variable.other.type",
        "variable.other.generic",
        "entity.name.type.type-parameter",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#c191ff"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#D5B778"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator": "#7488C3",
    "stringLiteral": "#6CAB74",
    "customLiteral": "#BDC0C9",
    "numberLiteral": "#BDC0C9"
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "import.alias:go": { "foreground": "#E1BFFF" }
    }
  }
}
